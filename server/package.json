    "jest": "^22.4.2",
    "moment": "^2.22.1",
    "moment-range": "^4.0.0",
    "mongoose": "^5.0.7",
    "nodemon": "^1.14.12",
    "npm": "^5.7.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "supertest": "^3.0.0"
  }
